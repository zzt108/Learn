@model Sitecore.Mvc.Presentation.RenderingModel
@{
    Layout = null;

}

<!DOCTYPE html>
<html>
<head>
    <title>@Model.Item.DisplayName - Launch Sitecore</title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <script type="text/javascript" src="/scripts/jquery-2.0.0.min.js"></script>
    <script type="text/javascript" src="/scripts/jquery.validate.min.js"></script>
    <script type="text/javascript" src="/_js/jqueryslidemenu.js"></script>
    <script type="text/javascript" src="/scripts/SearchTextBox.js"></script>
    <script type="text/javascript" src="/scripts/ContactForm.js"></script>
    <link href="/default.css" rel="stylesheet" />
    <link href="/_css/main.css" rel="stylesheet" type="text/css" /> 
    <link href="/_css/jqueryslidemenu.css" rel="stylesheet" type="text/css" /> 
    <link href="/_css/secnav.css" rel="stylesheet" type="text/css" />
    <link href="/CSS/ContactForm.css" rel="stylesheet" type="text/css" /> 

    <script src='/Scripts/MicrosoftAjax.debug.js'  type='text/javascript'></script>  
    <script src='/Scripts/MicrosoftMvcAjax.debug.js' type='text/javascript'></script>


</head>
<body>

<div id="SectionWrapper">
  <div id="wrapper">
   <!-- BEGIN: HEADER -->
   <div id="headerwrapper" lang="en">
    <h3 id="logo">
     <a href="/">Home</a>
    </h3>

    <!-- SearchForm -->
    <!-- /launchsitecore/mvc/search/SearchFormMini -->
    @Html.Sitecore().Rendering("{0F3283C7-9C7C-43DF-B993-079155299D19}",
                new { 
                    DataSource = "{ED2BEB41-F054-4D68-A99F-3FC333D76F3B}", 
                    Cacheable = false, 
                    CacheKey = "searchform_mini", 
                    Cache_VaryByData = true })

   </div>
   <div class="floatClear"></div>
   <!-- BEGIN: Top Nav -->
   
   <!-- Main Navigation -->
   @Html.Sitecore().Placeholder("main-navigation")
   @Html.Sitecore().Rendering("{B4805C27-293A-4C8B-B45A-67FB78EB7878}", new { 
    Cacheable = true, 
    CacheKey = "mainnav", 
    Cache_VaryByData = true })
 
       
   <!-- END: Top Nav -->
   <div id="container">

   <!-- breadcrumbs -->
   @Html.Sitecore().Placeholder("breadcrumbs")
   @Html.Sitecore().Rendering("{06D9F5DA-4372-4841-BA4F-946592EBDF9B}", new { 
    Cacheable = true, 
    CacheKey = "breadcrumbs", 
    Cache_VaryByData = true }) 
   
   <!-- BEGIN: Main Content Area -->
    <div id="contentwrapper">
        @Html.Sitecore().Placeholder("main-content")
    </div>
    <!-- END:  Main Content Area -->
    <div class="floatClear">
    </div>
   </div>        
   
   @Html.Sitecore().XsltRendering("/xsl/launchsitecore/default/footer.xslt", new { 
    Cacheable = true, 
    CacheKey = "footer" })


  </div>
 </div>


</body>
</html>
